{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nome do Data Factory",
			"defaultValue": "tokyo-olympics-df-luis"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Cadeia segura para \"accountKey\" de \"AzureDataLakeStorage1\""
		},
		"AthleteHttp_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/luispedro30/tokyo-olympics-data/master/Data/Athletes.csv"
		},
		"AthlethesHttp_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/luispedro30/tokyo-olympics-data/master/Data/Athletes.csv"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tokyoolympicdataluis.dfs.core.windows.net/"
		},
		"HttpCoaches_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/luispedro30/tokyo-olympics-data/master/Data/Coaches.csv"
		},
		"GenderHttp_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/luispedro30/tokyo-olympics-data/master/Data/EntriesGender.csv"
		},
		"HttpMedals_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/luispedro30/tokyo-olympics-data/master/Data/Medals.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI para conta de armazenamento contendo os modelos ARM ligados"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token sas para conta de armazenamento contendo os modelos ARM ligados"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "tokyo-olympics-df-luis_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AthleteHttp_properties_typeProperties_url": {
						"value": "[parameters('AthleteHttp_properties_typeProperties_url')]"
					},
					"AthlethesHttp_properties_typeProperties_url": {
						"value": "[parameters('AthlethesHttp_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"HttpCoaches_properties_typeProperties_url": {
						"value": "[parameters('HttpCoaches_properties_typeProperties_url')]"
					},
					"GenderHttp_properties_typeProperties_url": {
						"value": "[parameters('GenderHttp_properties_typeProperties_url')]"
					},
					"HttpMedals_properties_typeProperties_url": {
						"value": "[parameters('HttpMedals_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}